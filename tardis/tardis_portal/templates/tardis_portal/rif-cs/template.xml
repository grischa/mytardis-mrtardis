<?xml version="1.0" encoding="UTF-8"?>
{% load xmldate %}
<registryObjects xmlns="http://ands.org.au/standards/rif-cs/registryObjects"
                xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:schemaLocation="http://ands.org.au/standards/rif-cs/registryObjects http://services.ands.org.au/home/orca/schemata/registryObjects.xsd">

		<registryObject
			group="Monash University">
		<key>MONASH:eif019</key>
		<originatingSource>http://www.monash.edu.au</originatingSource>
		<collection type="registry" dateAccessioned="{{now|toxmldatetime}}"  dateModified="{{now|toxmldatetime}}">
		<identifier type="local">MONASH:eif019</identifier>
		<location>
		<address><electronic type="url"> <value>http://www.monash.edu.au</value>
		</electronic> <physical> <addressPart type="Monash University"></addressPart>
		</physical></address>
		</location>	
		<description type="about">
			Monash University EIF019 Metadata Store
		</description>
		</collection>
		</registryObject>

		{% for experiment in experiments %}
		{{ experiment.rif_cs_profile }}
			{% include experiment.rif_cs_profile %}
		{% endfor %}
</registryObjects>
